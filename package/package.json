{
	"name": "callskit",
	"version": "0.0.1",
	"description": "A toolkit for building call experience using Cloudflare Realtime",
	"type": "module",
	"sideEffects": false,
	"main": "dist/index.cjs",
	"module": "dist/index.js",
	"types": "dist/index.d.ts",
	"author": {
		"name": "Vaibhav Shinde",
		"email": "vaibhavkshinde20@gmail.com",
		"url": "https://github.com/vaibhavshn"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/vaibhavshn/callskit.git"
	},
	"exports": {
		".": {
			"require": {
				"types": "./dist/index.d.cts",
				"default": "./dist/index.cjs"
			},
			"import": {
				"types": "./dist/index.d.ts",
				"default": "./dist/index.js"
			}
		},
		"./react": {
			"require": {
				"types": "./dist/react.d.cts",
				"default": "./dist/react.cjs"
			},
			"import": {
				"types": "./dist/react.d.ts",
				"default": "./dist/react.js"
			}
		},
		"./server": {
			"types": "./dist/server.d.ts",
			"default": "./dist/server.js"
		}
	},
	"files": [
		"dist"
	],
	"scripts": {
		"build": "NODE_ENV=production tsup",
		"build:watch": "tsup --watch --sourcemap",
		"dev": "tsup --watch --sourcemap"
	},
	"dependencies": {
		"partysocket": "^1.1.4",
		"partytracks": "^0.0.49",
		"rxjs": "^7.8.2",
		"tiny-invariant": "^1.3.3"
	},
	"devDependencies": {
		"@types/lodash-es": "^4.17.12",
		"lodash-es": "^4.17.21",
		"tsup": "^8.5.0"
	},
	"peerDependencies": {
		"partykit": "^0.0.115"
	}
}
